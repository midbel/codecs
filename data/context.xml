<?xml version="1.0" encoding="utf-8"?>

<angle type="query" version="1">
	<base-uri>http://midbel.org</base-uri>

	<extensions>
		<extension>file</extension>
		<extension>http</extension>
		<extension>hash</extension>
	</extensions>

	<namespaces enforce="true">
		<namespace target="element"></namespace>
		<namespace target="type"></namespace>
		<namespace target="function"></namespace>
		<namespace prefix="angle">http://midbel.org/angle</namespace>
		<namespace prefix="sweet">http://midbel.org/sweet</namespace>
	</namespaces>

	<variables>
		<variable name="name">foobar</variable>
		<variable name="host">midbel</variable>
		<variable name="version">3.0</variable>
	</variables>
	
</angle>